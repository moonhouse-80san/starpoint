<query id="getRatingList" action="select">
	<tables>
		<table name="document_star" alias="ds" />
		<table name="member" alias="m" type="left join">
			<conditions>
				<condition operation="equal" column="ds.member_srl" default="m.member_srl" />
			</conditions>
		</table>
	</tables>
	<columns>
		<column name="ds.*" />
		<column name="m.user_id" />
		<column name="m.user_name" />
		<column name="m.nick_name" />
	</columns>
	<conditions>
		<condition operation="equal" column="ds.document_srl" var="document_srl" filter="number" notnull="notnull" />
	</conditions>
	<navigation>
		<index var="sort_index" default="ds.regdate" order="desc" />
		<list_count var="list_count" default="100" />
	</navigation>
</query>